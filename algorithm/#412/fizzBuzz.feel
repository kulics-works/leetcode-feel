FizzBuzz := (n : Int -> v : List[Str]) {
    arr := List_of[Str]()

    @ i := 1.Up_to(n)... {
        ? i % 15 == 0 {
            arr.Append("FizzBuzz")
        } | i % 3 == 0 {
            arr.Append("Fizz")
        } | i % 5 == 0 {
            arr.Append("Buzz")
        } | {
            arr.Append(i.To_Str())
        }
    }
    <- arr
}
