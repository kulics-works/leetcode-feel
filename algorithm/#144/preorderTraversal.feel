` Definition for a binary tree node. `
TreeNode = $ (
    val: Int,
    left: TreeNode?,
    right: TreeNode?
)

PreorderTraversal = (root: TreeNode?->v: (Int)List) {
    ? root == nil {
        <- nil
    }
    arr = (Int)List{}
    arr.Append(root.val)
    temp = PreorderTraversal(root.left)
    ? temp <> nil {
        arr.Append(temp)
    }
    temp = PreorderTraversal(root.right)
    ? temp <> nil {
        arr.Append(temp)
    }
    <- arr
}
