FlipAndInvertImage = (a: ((Int)List)List -> v: ((Int)List)List) {
    Reverse = (numbers: (Int)List -> v: (Int)List) {
        ret = (Int)List_of()
        @ numbers.Size() - 1 ... 0 ~ -1 => i {
            ret += 1 - numbers[i]
        }
        <- ret
    }

    @ a => [i]v {
        a[i] = Reverse(v)
    }
    <- a
}
