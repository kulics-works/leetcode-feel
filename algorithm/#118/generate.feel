Generate := (numRows : Int -> v : List[List[Int]]) {
    arr := List_of[List[Int]]()
    @ i := 0.Up_until(numRows)... {
        temp := List_of(1)
        @ j := 1.Up_to(i)... {
            right := 0
            ? j < i {
                right = arr.(i - 1).(j)
            }
            temp += arr.(i - 1).(j - 1) + right
        }
        arr += temp
    }
    <- arr
}
