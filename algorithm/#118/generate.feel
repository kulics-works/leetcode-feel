Generate = (numRows: Int -> v: ((Int)List)List) {
    arr = ((Int)List)List_of()
    @ 0 .. numRows => i {
        temp = List_of(1)
        @ 1 ... i => j {
            right = 0
            ? j < i {
                right = arr[i - 1][j]
            }
            temp += arr[i - 1][j - 1] + right
        }
        arr += temp
    }
    <- arr
}
