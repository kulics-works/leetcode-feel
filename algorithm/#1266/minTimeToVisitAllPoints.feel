minTimeToVisitAllPoints = (points: ((Int)List)List->v: Int) {
    length = 0
    @ 0 .. points.Size() => i {
        lx = points[i][0] - points[i + 1][0]
        ly = points[i][1] - points[i + 1][1]
        ? lx < 0 {
            lx = -lx
        }
        ? ly < 0 {
            ly = -ly
        }
        ? lx > ly {
            length += lx
        } | {
            length += ly
        }
    }
    <- length
}
