minTimeToVisitAllPoints := (points : List[List[Int]]->v : Int) {
    length := 0
    @ i := 0.Up_until(points.Size())... {
        lx := points.Get(i).Get(0) - points.Get(i + 1).Get(0)
        ly := points.Get(i).Get(1) - points.Get(i + 1).Get(1)
        ? lx < 0 {
            lx = -lx
        }
        ? ly < 0 {
            ly = -ly
        }
        ? lx > ly {
            length += lx
        } | {
            length += ly
        }
    }
    <- length
}
