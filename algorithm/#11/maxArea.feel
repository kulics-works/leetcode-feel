MaxArea := (height : List[Int]->v : Int) {
    topValue := 0
    l, r := 0, height.Size() - 1
    @ l < r {
        value = Min(height.Get(l), height.Get(r)) * (r - l)
        topValue = Max(value, topValue)
        ? height.Get(l) > height.Get(r) {
            r -= 1
        } | {
            l += 1
        }
    }
    <- topValue
}
