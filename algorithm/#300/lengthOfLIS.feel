lengthOfLIS := (nums : List[Int]->v : Int) {
    ? nums.Size() == 0 {
        <- 0
    }
    dp := List[Int]$(nums.Size())
    maxLength := 1
    dp.Set(0, 1)
    @ i := 1.Up_until(nums.Size())... {
        maxValue := 0
        @ j := 0.Up_until(i)... {
            ? nums.Get(i) > nums.Get(j) {
                maxValue = Max(maxValue, dp.Get(j))
            }
        }
        dp.Set(i, maxValue + 1)
        maxLength = Max(maxLength, dp.Get(i))
    }
    <- maxLength
}
