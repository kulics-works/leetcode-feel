lengthOfLIS = (nums: (Int)List->v: Int) {
    ? nums.Size() == 0 {
        <- 0
    }
    dp = (Int)New_List(nums.Size())
    maxLength = 1
    dp[0] = 1
    @ 1 .. nums.Size() => i {
        maxValue = 0
        @ 0 .. i => j {
            ? nums[i] > nums[j] {
                maxValue = Max(maxValue, dp[j])
            }
        }
        dp[i] = maxValue + 1
        maxLength = Max(maxLength, dp[i])
    }
    <- maxLength
}
