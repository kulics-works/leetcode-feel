lengthOfLIS := (nums: [list int]->v: int) {
    ? nums.len == 0 {
        <- 0
    }
    dp := [list int](nums.len)
    !maxLength := 1
    dp[0] = 1
    @ i := 1 .. nums.len {
        !maxValue := 0
        @ j := 0 ..< i {
            ? nums[i] > nums[j] {
                maxValue = max(maxValue, dp[j])
            }
        }
        dp[i] = maxValue + 1
        maxLength = max(maxLength, dp[i])
    }
    <- maxLength
}