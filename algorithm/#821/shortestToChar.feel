ShortestToChar = (s: Str, c: Chr -> v: (Int)List) {
    flag = -s.Size()
    ret = (Int)New_List(s.Size())
    @ s => [i]v {
        ? v == c {
            flag = i
        }
        ret[i] = i - flag
    }
    flag = s.Size() * 2
    @ s.Size() - 1 ... 0 ~ -1 => i {
        ? s[i] == c {
            flag = i
        }
        ret[i] = Min(flag - i, ret[i])
    }
    <- ret
}
