let twoSum(nums List[Int]) List[Int] = {
    let map = hashMapOf[Int, Int]();
    for nums.withIndex() as (i, v) then {
        let temp = target - v;
        if map[target - v] as Some(v) then {
            return listOf[Int](i, v);
        } else {
            map[v] = i;
        };
    };
    listOf[Int]()
};
