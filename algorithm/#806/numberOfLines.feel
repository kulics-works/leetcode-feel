NumberOfLines = (widths: (Int)List, s: Str -> v: (Int)List) {
    count = 0
    line = 1

    @ s => ea {
        width = widths[ea - 'a']
        ? width + count > 100 {
            line += 1
            count = width
        } | {
            count += width
        }
    }
    <- List_of(line, count)
}
