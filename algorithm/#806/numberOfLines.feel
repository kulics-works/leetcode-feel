NumberOfLines := (widths : List[Int], s : Str -> v : List[Int]) {
    count := 0
    line := 1

    @ it := s... {
        width := widths.(it - 'a')
        ? width + count > 100 {
            line += 1
            count = width
        } | {
            count += width
        }
    }
    <- List_of(line, count)
}
