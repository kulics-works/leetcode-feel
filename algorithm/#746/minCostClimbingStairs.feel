MinCostClimbingStairs := (cost : List[Int]->v : Int) {
    ? cost.Size() == 0 {
        <- 0
    }
    dp = List[Int]$(cost.Size())
    dp.(0) = cost.(0)
    dp.(1) = cost.(1)
    @ i := 2.Up_until(cost.Size())... {
        dp.(i) = cost.(i) + Min(dp.(i - 1), dp.(i - 2))
    }
    <- Min(dp.(cost.Size() - 1), dp.(cost.Size() - 2))
}
