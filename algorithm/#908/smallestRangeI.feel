SmallestRangeI := (a : List[Int], k : Int -> v : Int) {
    ? a.Size() == 0 {
        <- 0
    }
    max := a.(0)
    min := a.(0)
    @ it := a... {
        ? it > max {
            max = it
        } | ea < min {
            min = it
        }
    }
    ? max - min >= k * 2 {
        <- max - min - k * 2
    }
    <- 0
}
