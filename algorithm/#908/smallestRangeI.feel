SmallestRangeI = (a: (Int)List, k: Int -> v: Int) {
    ? a.Size() == 0 {
        <- 0
    }
    max = a[0]
    min = a[0]
    @ a => ea {
        ? ea > max {
            max = ea
        } | ea < min {
            min = ea
        }
    }
    ? max - min >= k * 2 {
        <- max - min - k * 2
    }
    <- 0
}
