` Definition for a binary tree node. `
TreeNode = $ (
    val: Int,
    left: TreeNode?,
    right: TreeNode?
)

PostorderTraversal = (root: TreeNode?->v: (Int)List) {
    ? root == nil {
        <- nil
    }
    arr = (Int)List_of()
    temp = PostorderTraversal(root.left)
    ? temp <> nil {
        arr.Append(temp)
    }
    temp = PostorderTraversal(root.right)
    ? temp <> nil {
        arr.Append(temp)
    }
    arr.Append(root.val)
    <- arr
}
