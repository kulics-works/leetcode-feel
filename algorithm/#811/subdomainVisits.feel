SubdomainVisits = (cpdomains: list`str -> v: list`str) {
    !Result = list`str{}
    !TempDic = dict`str`int{}
    cpdomains @ ea {
        !temp = ea.split(" ")
        !count = temp[0].to_i32()
        !arr = temp[1].split(".")
        arr @ [i]v {
            records := arr[i..].join(".")
            TempDic[records] += count
        }
    }
    TempDic @ [k]v {
        Result += v.to_str() + " " + k
    }
    <- Result
}
