SubdomainVisits = (cpdomains: (Str)List -> v: (Str)List) {
    ret = (Str)List_of()
    temp_dict = (Str, Int)Dict_of()
    @ cpdomains => ea {
        temp = ea.Split(" ")
        count = temp[0].To_I32()
        arr = temp[1].Split(".")
        @ arr => [i]v {
            records = arr[i..].Join(".")
            temp_dict[records] += count
        }
    }
    @ temp_dict => [k]v {
        ret += v.To_Str() + " " + k
    }
    <- ret
}
