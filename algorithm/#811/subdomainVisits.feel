SubdomainVisits := (cpdomains : List[Str] -> v : List[Str]) {
    ret := List_of[Str]()
    temp_dict := Dict_of[Str, Int]()
    @ ea := cpdomains... {
        temp := ea.Split(" ")
        count := temp.Get(0).To_I32()
        arr := temp.Get(1).Split(".")
        @ i, v := arr... {
            records := arr.Slice(i, nil).Join(".")
            temp_dict[records] += count
        }
    }
    @ k, v := temp_dict... {
        ret += v.To_Str() + " " + k
    }
    <- ret
}
