Convert = (s: Str, numRows: Int->v: Str) {
    ? numRows == 1 {
        <- s
    }
    res = ((Byte)List)List(numRows)
    direct = true
    n = 0
    @ s => v {
        res[n] += v
        ? n == numRows - 1 {
            direct = false
        } | n == 0 {
            direct = true
        }
        ? direct {
            n += 1
        } | {
            n -= 1
        }
    }
    @ 1 .. numRows => i {
        res[0] += res[i]
    }
    <- res[0].To_Str()
}
